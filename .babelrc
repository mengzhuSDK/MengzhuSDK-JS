{
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-0",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-decorators-legacy",
      "transform-class-properties",
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
  